<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.example.tsypenkovalaba101112.HelloController"
      prefWidth="1000.0">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Button text="Настройка подключения" onAction="#onSettingsButtonClick" styleClass="button"/>
    <Button text="Таблица детей" onAction="#onViewChildren" styleClass="button"/>
    <Button text="Таблица родителей" onAction="#onViewParents" styleClass="button"/>
</VBox>